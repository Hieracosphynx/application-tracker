{
  "name": "application-tracker",
  "version": "1.0.0",
  "description": "An application for tracking job applications",
  "main": "server.js",
  "scripts": {
    "node": "node server.js",
    "server": "nodemon --exec ./node_modules/.bin/babel-node server.js",
    "client": "npm start --prefix client",
    "client:install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
