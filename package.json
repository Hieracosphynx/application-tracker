  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.14.9",
    "bcrypt": "^5.0.1",
    "concurrently": "^6.2.1",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.2",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0"
  }
}
